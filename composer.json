{
  "name": "utexas/pantheon_saml_integration",
  "type": "composer-plugin",
  "description": "Composer plugin that integrates the University of Texas Enterprise Authentication on Pantheon for Wordpress",
  "version": "wp",
  "keywords": [
    "Wordpress"
  ],
  "license": [
    "GPL-2.0-or-later"
  ],
  "homepage": "https://drupalkit.its.utexas.edu",
  "minimum-stability": "dev",
  "authors": [
    {
      "name": "Web Content Management Solutions Team, Information Technology Services, The University of Texas at Austin",
      "email": "drupal-kit-support@utlists.utexas.edu",
      "homepage": "https://drupalkit.its.utexas.edu",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://github.austin.utexas.edu/eis1-wcs/pantheon_saml_integration/issues",
    "source": "https://github.austin.utexas.edu/eis1-wcs/pantheon_saml_integration"
  },
  "require": {
    "composer-plugin-api": "^1.0 || ^2.0",
    "composer/installers": "^1.9 || ^2.0",
    "cweagans/composer-patches": "^1.7",
    "pantheon-systems/wp-saml-auth": "2.1.1",
    "simplesamlphp/composer-module-installer": "1.2.0",
    "simplesamlphp/saml2": "4.6.8",
    "simplesamlphp/simplesamlphp": "1.19.8"
  },
  "require-dev": {
    "composer/composer": "^1.0 || ^2.0"
  },
  "autoload": {
    "psr-4": {
      "Utexas\\Composer\\": "src/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "class": "Utexas\\Composer\\SamlAuthenticationSetupPlugin",
    "patches": {
      "pantheon-systems/wp-saml-auth": {
          "Allow for IdP logout": "https://drupalkit.its.utexas.edu/patches/wp-saml-auth_logout.patch"
      }
    }
  },
  "prefer-stable": true
}
